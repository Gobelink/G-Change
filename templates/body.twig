<div id="body-container">
	{% if module is defined %}
		{% if module == 'customers' %}
			{% include 'customers-form.twig' %}
		{% elseif module == 'orders' %}
			{% include 'orders-form.twig' %}
		{% elseif module == 'products' %}
			{% include 'products-form.twig' %}
		{% endif %}
	{% endif %}
</div>